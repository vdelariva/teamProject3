    "body-parser": "^1.17.2",
    "dotenv": "^6.0.0",
    "express": "^4.15.4",
    "if-env": "^1.0.4",
    "mongoose": "^5.0.17",
    "morgan": "^1.9.1",
    "nodemailer": "^4.6.8",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
